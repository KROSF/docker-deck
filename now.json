{
  "name": "docker-deck",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/[0-9]+$",
      "dest": "/"
    }
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "public": true
}